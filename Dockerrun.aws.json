{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "dmbondarenko/multi_container_client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "dmbondarenko/multi_container_api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "dmbondarenko/multi_container_worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "dmbondarenko/multi_container_server",
      "essential": true,
      "memory": 128,
      "portMappings": [{
        "hostPort": 80,
        "containerPort": 80
      }],
      "links": [
        "client",
        "server"
      ]
    }
  ]
}
